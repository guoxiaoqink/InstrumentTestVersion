<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout android:layout_width="match_parent"
        android:layout_height="@dimen/dp113"
        android:orientation="vertical">
        <LinearLayout android:layout_width="@dimen/dp_18"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                <com.example.tu4.view.CircleImageView
                        android:id="@+id/circleimage"
                        android:layout_width="@dimen/dp_14"
                        android:layout_height="@dimen/dp_14"
                    android:layout_centerHorizontal="true"
                        android:src="@mipmap/colorcccccc"
                        app:border_color="@android:color/transparent"
                    app:border_width="@dimen/px0"
                />
            </RelativeLayout>

            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                <TextView
                        android:id="@+id/textview_verticalline"
                        android:layout_width="@dimen/dp1"
                        android:layout_height="match_parent"
                    android:layout_centerHorizontal="true"
                        android:background="@color/color_cccccc"
                />
            </RelativeLayout>

        </LinearLayout>

        <TextView
                android:id="@+id/textview_content"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_33"
            android:text="@string/logististrackinglistviewitem"
                android:textColor="@color/color_999999"
            android:textSize="@dimen/sp_14"
        />
        <TextView
                android:id="@+id/textview_time"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
            android:layout_below="@+id/textview_content"
            android:layout_marginLeft="@dimen/dp_33"
            android:layout_marginTop="@dimen/dp10"
            android:text="2016.06.12 10:15"
            android:textColor="@color/color_cccccc"
            android:textSize="@dimen/sp_12"

        />


    </RelativeLayout>

</RelativeLayout>
