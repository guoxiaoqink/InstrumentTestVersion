<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_ffffff"
    android:orientation="vertical"
    tools:context="com.example.tu4.activity.SearchActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp44"
        android:background="@color/s97c8cd">

        <ImageView
            android:id="@+id/img_actionbar_serach"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp10"
            android:src="@mipmap/left_arrow_white" />


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_34"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginRight="@dimen/dp_15"
            android:layout_toLeftOf="@+id/imageview_instrument_show"
            android:layout_toRightOf="@+id/img_actionbar_serach"
            android:background="@drawable/bg_topbar_edit">

            <ImageView
                android:id="@+id/find"
                android:layout_width="@dimen/dp_17"
                android:layout_height="@dimen/dp_17"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp5"
                android:src="@mipmap/lookup_serach" />

            <EditText
                android:textColor="@color/color_333333"
                android:id="@+id/edt_topbar_serach"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp5"
                android:layout_toRightOf="@+id/find"
                android:background="@null"
                android:hint="@string/importdouble"
                android:textColorHint="@color/color_333333"
                android:textSize="@dimen/sp14" />
        </RelativeLayout>
        <!-- <EditText
                 android:layout_width="wrap_content"
                 android:text="@string/instrument"
                 android:gravity="center"
                 android:textColor="@color/sffffff"
                 android:textSize="@dimen/px34"
                 android:layout_height="match_parent"
                 android:layout_alignParentTop="true"
                 android:layout_centerHorizontal="true"/>-->
        <TextView
            android:id="@+id/imageview_instrument_show"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp10"
            android:text="@string/serach"
            android:textColor="@color/color_ffffff"
            android:textSize="@dimen/sp14" />
    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <!--主menu-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/linearlayout_topmenu_subject"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/img_topmenu_subject"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/dp0"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/dp15"
                        android:layout_weight="1"
                        android:src="@mipmap/serach_topmenu_subject" />

                    <TextView
                        android:id="@+id/textview_topmenu_subject"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_marginBottom="@dimen/dp5"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/subject"
                        android:textColor="@color/color_666666"
                        android:textSize="@dimen/sp_12" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearlayout_topmenu_instrument"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/img_topmenu_instrument"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/dp0"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/dp15"
                        android:layout_weight="1"
                        android:src="@mipmap/serach_topmenu_instrument" />

                    <TextView
                        android:id="@+id/textview_topmenu_instrument"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_marginBottom="@dimen/dp5"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/instrument"
                        android:textColor="@color/color_666666"
                        android:textSize="@dimen/sp_12" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearlayout_topmenu_order"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/img_topmenu_order"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/dp0"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/dp15"
                        android:layout_weight="1"
                        android:src="@mipmap/search_topmenu_order" />

                    <TextView
                        android:id="@+id/textview_topmenu_order"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_marginBottom="@dimen/dp5"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/order"
                        android:textColor="@color/color_666666"
                        android:textSize="@dimen/sp_12"

                        />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearlayout_topmenu_record"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/img_topmenu_record"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/dp0"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/dp15"
                        android:layout_weight="1"
                        android:src="@mipmap/serach_topmenu_record" />

                    <TextView
                        android:id="@+id/textview_topmenu_record"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_marginBottom="@dimen/dp5"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/transactionrecord"
                        android:textColor="@color/color_666666"
                        android:textSize="@dimen/sp_12" />
                </LinearLayout>
            </LinearLayout>
            <!--主menufrfrfrfrfrfrf-->
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp5"
                android:background="@drawable/dividebg" />
            <!--课程-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/color_ffffff"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/dp5"
                    android:layout_marginLeft="@dimen/dp10"
                    android:layout_marginTop="@dimen/dp5"
                    android:text="@string/subject"
                    android:textColor="@color/color_666666"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/px1"
                    android:background="@color/color_d1d1d1" />

                <com.example.tu4.view.ResolveConflictsScoolviewListview
                    android:id="@+id/listview_subject_serach"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:divider="@drawable/serach_subject_listitem_divider"
                    android:dividerHeight="@dimen/px1"></com.example.tu4.view.ResolveConflictsScoolviewListview>
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp5"
                android:background="@drawable/dividebg" />
            <!--预约订单-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/color_ffffff"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/dp5"
                    android:layout_marginLeft="@dimen/dp10"
                    android:layout_marginTop="@dimen/dp5"
                    android:text="@string/order"
                    android:textColor="@color/color_666666"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/px1"
                    android:background="@color/color_d1d1d1" />

                <com.example.tu4.view.ResolveConflictsScoolviewListview
                    android:id="@+id/listview_order_serach"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:divider="@drawable/serach_subject_listitem_divider"
                    android:dividerHeight="@dimen/px1"></com.example.tu4.view.ResolveConflictsScoolviewListview>
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp5"
                android:background="@drawable/dividebg" />
            <!--交易记录-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/color_ffffff"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/dp5"
                    android:layout_marginLeft="@dimen/dp10"
                    android:layout_marginTop="@dimen/dp5"
                    android:text="@string/transactionrecord"
                    android:textColor="@color/color_666666"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/px1"
                    android:background="@color/color_d1d1d1" />

                <com.example.tu4.view.ResolveConflictsScoolviewListview
                    android:id="@+id/listview_record_serach"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:divider="@drawable/serach_subject_listitem_divider"
                    android:dividerHeight="@dimen/px1"></com.example.tu4.view.ResolveConflictsScoolviewListview>
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp5"
                android:background="@drawable/dividebg" />
            <!--乐器-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/color_ffffff"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/dp5"
                    android:layout_marginLeft="@dimen/dp10"
                    android:layout_marginTop="@dimen/dp5"
                    android:text="@string/instrument"
                    android:textColor="@color/color_666666"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/px1"
                    android:background="@color/color_d1d1d1" />

                <com.example.tu4.view.ResolveConflictsScoolviewGridview
                    android:id="@+id/gridview_serach"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/instrument_instrument_gridviewToleft"
                    android:layout_marginRight="@dimen/instrument_instrument_gridviewToright"
                    android:layout_marginTop="@dimen/instrument_instrument_gridviewTotop"
                    android:gravity="center"
                    android:horizontalSpacing="@dimen/instrument_instrument_griview_horizontal"
                    android:numColumns="2"
                    android:stretchMode="columnWidth"
                    android:verticalSpacing="@dimen/instrument_instrument_gridview_verticalSpacing" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>